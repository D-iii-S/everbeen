# Configuration:error <- 0.975 # 95% confidence intervalsfile.name <- "graph.png"# read the data filedata <- read.csv("data.csv", header=TRUE, sep=";")#colnames(data) <- c("requests","served")result <- data.frame(requests=unique(data$requests))result$mean <- result$requestsfor (i in 1:length(result$requests)){	current.data <- data$served[data$requests==result$requests[i]]	current.count <- length(current.data)	current.mean <- mean(current.data)	result$mean[i] <- current.mean}################################################################################# Initialize plotpng(file.name, width=600, height=300)par(mar=c(3,3,2,0))   # margin - SWNEpar(oma=c(0,0,0,0))   # outer margin - SWNEpar(mgp=c(1.5,0.2,0)) # Set margin lines; default c(3,1,0) [title,labels,line]par(tcl=0.35)         # Switch tick marks to insides of axespar(cex=0.75)         # font size (relative to default)par(lwd=2)# Define plot limits based on range of data to be plotted:xlim <- range(result$requests)ylim <- range(data$served)#--Define axis labels:xlab <- "Requested RPS"ylab <- "Served RPS"plot(data, col='grey', lwd=1, xlim=xlim, ylim=ylim, xlab=xlab, ylab=ylab)title("Overall Server Performance")lines(x=c(0,max(result$requests)), y=c(0,max(result$requests)), col='blue', lwd=1)lines(result$requests, result$mean, col='red', lwd=4)dev.off()################################################################################