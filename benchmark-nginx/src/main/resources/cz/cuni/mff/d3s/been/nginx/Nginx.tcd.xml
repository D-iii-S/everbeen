<?xml version="1.0"?>
<taskContextDescriptor xmlns="http://been.d3s.mff.cuni.cz/taskmanager/tcd"
                       xmlns:td="http://been.d3s.mff.cuni.cz/taskmanager/td"
                       name="nginx-context">

	<properties>
		<property name="revision">4532</property>
		<property name="numberOfClients">2</property>
	</properties>

	<task>
		<name>server</name>

		<properties>
			<property name="svnPath">svn://svn.nginx.org/nginx/trunk</property>
		</properties>

		<descriptor>
			<td:taskDescriptor
					groupId="cz.cuni.mff.d3s.been.nginx" bpkId="nginx-server"
					version="0.0.1" name="nginx-server">
			</td:taskDescriptor>
		</descriptor>
	</task>

	<task>
		<name>client-1</name>
		<properties>
			<property name="client-number">1</property>
		</properties>
		<descriptor>
			<fromTemplate>nginx-client</fromTemplate>
		</descriptor>
	</task>

	<task>
		<name>client-2</name>
		<descriptor>
			<fromTemplate>nginx-client</fromTemplate>
		</descriptor>
		<properties>
			<property name="client-number">2</property>
		</properties>
	</task>

	<templates>
		<template name="nginx-client">
			<td:taskDescriptor xmlns="http://been.d3s.mff.cuni.cz/taskmanager/td"
			                   groupId="cz.cuni.mff.d3s.been.nginx" bpkId="nginx-client"
			                   version="0.0.1" name="nginx-client">
				<td:properties>
					<td:property name="constProperty">value</td:property>
				</td:properties>
			</td:taskDescriptor>
		</template>
	</templates>

</taskContextDescriptor>
