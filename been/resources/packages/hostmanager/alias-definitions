<os type="cz.cuni.mff.been.hostmanager.database.RSLRestriction">
<rslRestriction>family == "Windows"</rslRestriction>
</os>
<app type="cz.cuni.mff.been.hostmanager.database.RSLRestriction">
<rslRestriction>name =~ /.*\.net framework.*/i &amp;&amp; vendor =~ /.*Microsoft.*/</rslRestriction>
</app>
</aliasDefinition>
