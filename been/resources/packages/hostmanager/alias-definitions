<?xml version="1.0" encoding="UTF-8"?>
<aliasList>
<aliasDefinition alias="Microsoft Visual Studio" name="${name}" vendor="${vendor}" version="${version}">
<os type="cz.cuni.mff.been.hostmanager.database.RSLRestriction">
<rslRestriction>family == "Windows"</rslRestriction>
</os>
<app type="cz.cuni.mff.been.hostmanager.database.RSLRestriction">
<rslRestriction>name =~ /.*microsoft.*visual.*studio.*/i</rslRestriction>
</app>
</aliasDefinition>
