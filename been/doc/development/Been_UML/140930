    class 205442 "CreatePackageMetadata"
      visibility public 
      cpp_decl ""
      java_decl "${comment}${@}${visibility}${final}${abstract}class ${name}${extends}${implements} {
${members}}
"
      php_decl ""
      python_2_2 python_decl ""
      idl_decl ""
      explicit_switch_type ""
      
      comment "
<p>Creates metadata file for a BEEN package in its working directory.</p>

<b>Task properties:</b><br>
<ul>
<li>{@value #PACKAGE_NAME}: <br>
		- name of the package<br>
<li>{@value #PACKAGE_HUMAN_NAME}: <br>
		- human readable, or a more descriptive package name<br>
<li>{@value #VERSION}: <br>
		- version of the package<br>
<li>{@value #HARDWARE_PLATFORMS}: <br>
		- hardware platforms supported by the package, separated by spaces<br>
<li>{@value #SOFTWARE_PLATFORMS}: <br>
		- software platforms supported by the package, separated by spaces<br>
<li>{@value #TYPE}: <br>
		- type of the package, valid values are {@value #TYPE_BINARY},
		 {@value #TYPE_SOURCE}, {@value #TYPE_DATA} and {@value #TYPE_TASK}<br>
</ul>

@author Jaroslav Urban
"
      classrelation 264194 // <generalisation>
